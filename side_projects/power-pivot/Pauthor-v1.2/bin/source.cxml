    <Item Id="1966" Name="Budowanie projektu ASP.NET z FAKE" Href="http://www.maciejaniserowicz.com/post/2010/11/25/Budowanie-projektu-ASPNET-z-FAKE.aspx" Img="Images\1966.png">
      <Facets>
        <Facet Name="Użytkownik">
          <String Value="macko" />
